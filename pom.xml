    <profiles>
        <profile>
            <id>dev</id>
            <activation>
                <property>
                    <name>profile</name>
                    <value>dev</value>
                </property>
                <activeByDefault>true</activeByDefault>
            </activation>
            <properties>
                <skip.assembly>true</skip.assembly>
                <skip.checkstyle>false</skip.checkstyle>
                <skip.findbugs>false</skip.findbugs>
                <skip.pmd>false</skip.pmd>
                <skipTests>false</skipTests>
            </properties>
        </profile>
        <profile>
            <id>rel</id>
            <activation>
                <property>
                    <name>profile</name>
                    <value>rel</value>
                </property>
            </activation>
            <properties>
                <skip.assembly>false</skip.assembly>
                <skip.checkstyle>false</skip.checkstyle>
                <skip.findbugs>false</skip.findbugs>
                <skip.pmd>false</skip.pmd>
                <skipTests>false</skipTests>
            </properties>
        </profile>
        <profile>
            <id>js</id>
            <activation>
                <property>
                    <name>profile</name>
                    <value>js</value>
                </property>
            </activation>
            <properties>
                <skip.assembly>true</skip.assembly>
                <skip.checkstyle>true</skip.checkstyle>
                <skip.findbugs>true</skip.findbugs>
                <skip.pmd>true</skip.pmd>
                <skipTests>true</skipTests>
            </properties>
        </profile>
        <profile>
            <id>heroku</id>
            <activation>
                <property>
                    <name>maven.repo.local</name>
                    <value>/app/tmp/cache/.m2/repository</value>
                </property>
            </activation>
            <properties>
                <skip.assembly>true</skip.assembly>
                <skip.checkstyle>true</skip.checkstyle>
                <skip.findbugs>true</skip.findbugs>
                <skip.pmd>true</skip.pmd>
                <skipTests>true</skipTests>
            </properties>
        </profile>
    </profiles>
